            /*salmonbites.style.display = "flex";*/
            piggytuna.style.display = "none";
            activehound.style.display = "flex";
            
          break;
          case "pollo":
            
            dataSuscribe.doNotDisplay = "activehound";
            /*salmonbites.style.display = "flex";*/
            piggytuna.style.display = "flex";
            activehound.style.display = "none";
            
          break;
          case "ninguno":
            
            dataSuscribe.doNotDisplay = "";
            /*salmonbites.style.display = "flex";*/
            piggytuna.style.display = "flex";
            activehound.style.display = "flex";
            
          break;
        }
        

        
        if(dataSuscribe.recomended == "piggytuna" && dataSuscribe.doNotDisplay == "piggytuna"){
          
          dataSuscribe.recomended = "activehound";
          
        }else if(dataSuscribe.recomended == "activehound" && dataSuscribe.doNotDisplay == "activehound"){
          
          dataSuscribe.recomended = "piggytuna";
          
        }else if(dataSuscribe.recomended == "salmonbites" && dataSuscribe.doNotDisplay == "salmonbites"){
          
          dataSuscribe.recomended = "activehound";
          
        }
        
        if(dataSuscribe.recomended == "piggytuna"){
          
          document.getElementById("suggestedPiggytuna").style.opacity = "1";
