          document.getElementById("suggestedSalmonbites").style.opacity = "0";
          
        }else if(dataSuscribe.recomended == "salmonbites"){
          
          document.getElementById("suggestedPiggytuna").style.opacity = "0";
          document.getElementById("suggestedActivehound").style.opacity = "0";
          document.getElementById("suggestedSalmonbites").style.opacity = "1";
          
        }
        
        
      }
      
