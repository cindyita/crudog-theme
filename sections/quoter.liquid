      function calculatePlan(){
        
        var total = Number(dataSuscribe.weight);
        
        var old = (Number(dataSuscribe.years) * 12) + Number(dataSuscribe.months);
        
        
        if(old <= 12) { /*menos de 12 meses*/

          total = total * 0.10; /* 10% */

        }else if(old >= 83) { /*más de 7 años*/

          total = total * 0.03; /* 3% */

        }else if(old > 12 && old < 83) { /*mas de 12 meses y menos de 7 años*/

          total = total * 0.04; /* 4% */

        }
        
        
        switch(dataSuscribe.activity){
          case "alto":
            dataSuscribe.recomended = "piggytuna";
          break;
          case "medio":
            total = total*0.9;
          break;
          case "bajo":
            total = total*0.8
            dataSuscribe.recomended = "activehound";
          break;
            default:
        }
        
        switch(dataSuscribe.perception){
          case "delgado":
            total = total*1.2;
          break;
          case "bien":
          break;
          case "rellenito":
           total = total*0.9;
          break;
          case "sobrepeso":
            total = total*0.8;
          break;
          default:
        }
        
        switch(dataSuscribe.alergy){          
          case "res":
           	dataSuscribe.salmonbright = false;
          break;
          case "cerdo":
            dataSuscribe.piggytuna = false;
          break;
          case "pollo":
           	dataSuscribe.activehound = false;
          break;
          case "ninguno":
          break;
           default:
        }
        
        if(dataSuscribe.plantype == "half"){
          total = total * 0.25;
        }else if (dataSuscribe.plantype == "full"){
          total = total * 0.5;
        }
        
		dataSuscribe.total = Math.round(total*1000);
           
        dataSuscribe.totalMonth = Math.ceil(total*30);
        
        
        if(dataSuscribe.plantype == "full"){
          
          document.getElementById("plan_txt").innerHTML = "Completo";
          
        }else if(dataSuscribe.plantype == "half"){
          
          document.getElementById("plan_txt").innerHTML = "Parcial";
          
        }
        
          document.getElementById("grDiarios_txt").innerHTML = dataSuscribe.total;
        
        for(i=1;i<=7;i++){
          document.getElementById("kgMes_txt" + i).innerHTML = dataSuscribe.totalMonth;
        }
        
      }
      
      
      function calculateProduct(){
       
